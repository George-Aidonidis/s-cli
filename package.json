{
	"name": "@george-aidonidis/s-cli",
	"repository": "george-aidonidis/s-cli",
	"version": "1.2.1",
	"license": "MIT",
	"main": "./src/s-cli.js",
	"scripts": {
		"start": "node ./src/s-cli",
		"test": "jest",
		"codecov": "codecov",
		"dev": "nodemon ./src/s-cli"
	},
	"bin": "src/s-cli.js",
	"dependencies": {
		"commander": "^6.2.0",
		"ramda": "^0.27.1",
		"shelljs": "^0.8.4"
	},
	"devDependencies": {
		"codecov": "^3.8.0",
		"husky": "^4.3.0",
		"jest": "^26.5.3",
		"lint-staged": "^10.4.0",
		"nodemon": "^2.0.4",
		"xo": "^0.33.1"
	},
	"lint-staged": {
		"*": [
			"xo --fix",
			"git add"
		]
	},
	"xo": {
		"prettier": true,
		"envs": [
			"jest",
			"node",
			"browser"
		],
		"rules": {
			"camelcase": 1
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "yarn test"
		}
	}
}
