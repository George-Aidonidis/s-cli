{
	"name": "@george-aidonidis/s-cli",
	"repository": "george-aidonidis/s-cli",
	"version": "0.2.0",
	"license": "MIT",
	"main": "./src/battery/index.js",
	"scripts": {
		"test": "jest",
		"codecov": "codecov",
		"dev": "nodemon ./src/battery/index.js"
	},
	"bin": {
		"battery-info": "./src/battery/index"
	},
	"dependencies": {
		"ramda": "^0.26.1",
		"shelljs": "^0.8.3"
	},
	"devDependencies": {
		"codecov": "^3.2.0",
		"husky": "^1.3.1",
		"jest": "^24.1.0",
		"lint-staged": "^8.1.4",
		"nodemon": "^1.18.10",
		"xo": "^0.24.0"
	},
	"lint-staged": {
		"*": [
			"xo --fix",
			"git add"
		]
	},
	"xo": {
		"prettier": true,
		"envs": [
			"jest",
			"node",
			"browser"
		],
		"rules": {
			"camelcase": 1
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "yarn test"
		}
	}
}
